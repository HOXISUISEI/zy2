<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5dc"
    >


    <com.google.android.material.chip.Chip
        android:id="@+id/chip9"
        android:layout_width="390dp"
        android:layout_height="51dp"
        android:layout_marginBottom="56dp"
        android:text="      收藏                                                                 62"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/chip5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/chip5"
        app:layout_constraintTop_toBottomOf="@+id/chip5"
        app:layout_constraintVertical_bias="0.0" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip7"
        android:layout_width="387dp"
        android:layout_height="48dp"
        android:layout_marginTop="56dp"
        android:text="     相册                                                                  35"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/re" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip5"
        android:layout_width="390dp"
        android:layout_height="51dp"
        android:layout_marginTop="12dp"
        android:text="     标签                                                                  7"
        app:layout_constraintEnd_toEndOf="@+id/chip4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/chip4"
        app:layout_constraintTop_toBottomOf="@+id/chip4" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip4"
        android:layout_width="390dp"
        android:layout_height="51dp"

        android:layout_marginTop="16dp"
        android:text="      喜欢                                                                325"
        app:layout_constraintEnd_toEndOf="@+id/chip7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/chip7"
        app:layout_constraintTop_toBottomOf="@+id/chip7" />

    <RelativeLayout
        android:id="@+id/re"
        android:layout_width="416dp"
        android:layout_height="268dp"
        android:layout_marginTop="4dp"
        android:background="#f0ffff"
        android:clickable="true"
        android:paddingTop="8dp"
        android:paddingBottom="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="104dp"
            android:layout_height="95dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="166dp"
            android:layout_marginRight="166dp"
            app:srcCompat="@drawable/_49px_" />

        <EditText
            android:id="@+id/editText2"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/editText"
            android:layout_alignLeft="@+id/editText"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="-4dp"
            android:layout_marginLeft="-4dp"
            android:layout_marginEnd="35dp"
            android:layout_marginRight="35dp"
            android:layout_marginBottom="25dp"
            android:background="@null"
            android:ems="10"
            android:hint="2333"
            android:inputType="textMultiLine|text|textPassword" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="34dp"
            android:layout_marginLeft="28dp"
            android:layout_marginBottom="25dp"
            android:gravity="center_vertical"
            android:text="233"
            android:textSize="18sp"
            android:verticalScrollbarPosition="defaultPosition" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textView2"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="37dp"
            android:layout_marginLeft="37dp"
            android:layout_marginBottom="7dp"
            android:gravity="center_vertical"
            android:text="点赞"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="179dp"
            android:layout_marginLeft="179dp"
            android:layout_marginTop="122dp"
            android:text="SST"
            android:textSize="18sp" />

        <EditText
            android:id="@+id/editText"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="174dp"
            android:layout_marginEnd="25dp"
            android:layout_marginRight="24dp"
            android:background="@null"
            android:ems="10"
            android:hint="访问"
            android:inputType="textMultiLine|textPersonName" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"

            android:layout_alignParentRight="true"
            android:layout_marginTop="152dp"
            android:layout_marginEnd="195dp"
            android:layout_marginRight="195dp"
            android:text="CHINA"
            android:textSize="18sp" />

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="7dp"
            android:layout_marginLeft="7dp"
            android:layout_marginTop="8dp"
            app:srcCompat="@drawable/abc_vector_test" />

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            app:srcCompat="@android:drawable/ic_dialog_email" />


    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>